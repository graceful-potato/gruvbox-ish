{
    "name": "gruvbox-ish",
    "publisher": "GracefulPotato",
    "repository": {
        "type": "git",
        "url": "https://github.com/graceful-potato/gruvbox-ish"
    },
    "homepage": "https://github.com/graceful-potato/gruvbox-ish",
    "displayName": "Gruvbox ish",
    "description": "Gruvbox-like theme.",
    "icon": "images/icon.png",
    "version": "0.7.0",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Themes"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:ruby"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "properties": {
                "gruvbox-ish.recommendRubyGrammar": {
                    "type": "boolean",
                    "default": true,
                    "description": "Recommend installing Better Ruby Highlighting."
                }
            }
        },
        "themes": [
            {
                "label": "Gruvbox ish Dark Soft",
                "uiTheme": "vs-dark",
                "path": "./themes/gruvbox-ish-dark-soft.json"
            },
            {
                "label": "Gruvbox ish Dark Medium",
                "uiTheme": "vs-dark",
                "path": "./themes/gruvbox-ish-dark-medium.json"
            },
            {
                "label": "Gruvbox ish Dark Hard",
                "uiTheme": "vs-dark",
                "path": "./themes/gruvbox-ish-dark-hard.json"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^22.10.0",
        "@types/vscode": "^1.41.0",
        "typescript": "^5.7.2"
    }
}
